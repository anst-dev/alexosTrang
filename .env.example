# ============================================
# Personal Dashboard - Environment Variables
# ============================================
# Copy this file to .env and fill in your values

# ============================================
# ACTIVE APIs - Quick Note Feature
# ============================================
# Chỉ 2 API này đang được sử dụng cho tính năng ghi chú nhanh

# API để gửi ghi chú nhanh
VITE_QUICK_NOTE_WEBHOOK_URL=
# API để lấy lịch sử ghi chú
VITE_GET_ALL_LOGS_URL=

# ============================================
# DISABLED APIs - Using localStorage
# ============================================
# Các API này đã bị vô hiệu hóa, ứng dụng sử dụng localStorage cho:
# - Goals (Mục tiêu)
# - Habits (Thói quen)
# - Journal (Nhật ký)
# - Milestones (Nhiệm vụ con)

# Storage Mode - FORCED to true
# Luôn sử dụng localStorage cho goals, habits, journal, milestones
VITE_USE_LOCAL_STORAGE=true
